{
  "name": "cheycron/laravel-menus",
  "description": "Laravel Menu management",
  "keywords": [
    "laravel",
    "menus",
    "navigation",
    "nav",
    "bootstrap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Cheycron Blaine",
      "email": "cheycron@gmail.com"
    },
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    },
    {
      "name": "Pingpong Labs",
      "email": "pingpong.labs@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "5.6.*",
    "illuminate/config": "5.6.*",
    "illuminate/view": "5.6.*",
    "laravelcollective/html": "5.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "3.6.*",
    "friendsofphp/php-cs-fixer": "^2.10"
  },
  "autoload": {
    "psr-4": {
      "Cheycron\\Menus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cheycron\\Menus\\Tests\\": "Tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    },
    "laravel": {
      "providers": [
        "Cheycron\\Menus\\MenusServiceProvider"
      ],
      "aliases": {
        "Menu": "Cheycron\\Menus\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "stable"
}
